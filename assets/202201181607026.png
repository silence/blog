                        type="text"
                        placeholder="任务名称…"/>
                <input  id="taskName2"
                        name="taskName2"
                        class="text taskName"
                        type="text"
                        placeholder="任务名称…"/>
                <input  id="taskName3"
                        name="taskName3"
                        class="text taskName"
                        type="text"
                        placeholder="任务名称…"/>
                <input  id="taskName4"
                        name="taskName4"
                        class="text taskName"
                        type="text"
                        placeholder="任务名称…"/>
                <input  id="taskName5"
                        name="taskName5"
                        class="text taskName"
                        type="text"
                        placeholder="任务名称…"/>
                <input  id="taskName6"
                        name="taskName6"
                        class="text taskName"
                        type="text"
                        placeholder="任务名称…"/>
                <input  id="taskName7"
                        name="taskName7"
                        class="text taskName"
                        type="text"
                        placeholder="任务名称…"/>
                <input  id="taskName8"
                        name="taskName8"
                        class="text taskName"
                        type="text"
                        placeholder="任务名称…"/>
                <input  id="taskName9"
                        name="taskName9"
                        class="text taskName"
                        type="text"
                        placeholder="任务名称…"/>
                <input  id="newTaskAssignee"
                        type="text"
                        class="text autocomplete-user"
                        name="assignee"
                        data-max="10"
                        data-template="{username}"
                        data-none-message="找不到匹配的用户"
                        placeholder="分配至…"/>
                <div class="usersdropdown aui-dd-parent">
                    <a href="" class="aui-dd-trigger" style="display:none;"></a>
                    <ul class="aui-dropdown aui-dropdown-left hidden">
                    </ul>
                </div>
                <ol class="assignees select single">
                </ol>
                <textarea class="textarea" placeholder="添加可选备注"/>
            </fieldset>
            <div class="button-panel">
                <a href="#" class="add-task">添加任务…</a>
                <input type="submit" name="create" class="create" value="创建">
                <a href="#" class="cancel-addtask">取消</a>
                <a href="#" class="cancel-dialog">取消</a>
            </div>
        </form>
    </div>
</script>

